{
  "builds":[{
    "src": "app/*",
    "use": "@vercel/python",
    "config": {
      "functions": {
        "app/api.py": {
          "maxDuration": 60
        }
      },
      "maxLambdaSize": "15mb"
    }
  }],
  "routes": [
    {
      "src":"/(.*)",
      "dest": "app/api.py"
    }
  ],
  "crons": [
    {
      "path": "/cron",
      "schedule": "11 12 * * *"
    },
    {
      "path": "/cron",
      "schedule": "11 10 * * *"
    }
  ]

}
