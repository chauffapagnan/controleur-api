{
  "functions": {
    "app/*": {
      "maxDuration": 60,
      "memory": 1024,
      "runtime": "python3.12"
    }
  },
  "routes": [
    {
      "src":"/(.*)",
      "dest": "app/api.py"
    }
  ],
  "crons": [
    {
      "path": "/cron",
      "schedule": "11 12 * * *"
    },
    {
      "path": "/cron",
      "schedule": "11 10 * * *"
    }
  ]


}
