{
  "builds":[{

    "src": "app/*",
    "use": "@vercel/python",
    "config": { "maxLambdaSize": "15mb" }
  }],
  "routes": [
    {
      "src":"/(.*)",
      "dest": "app/api.py"
    }
  ],
  "crons": [
    {
      "path": "/cron",
      "schedule": "14 13 * * *"
    },
    {
      "path": "/cron",
      "schedule": "14 18 * * *"
    }
  ]
}
